{
  "name": "@kitiumai/create-app",
  "version": "1.0.0",
  "description": "CLI tool for scaffolding KitiumAI projects",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-kitium-app": "./dist/cli.js",
    "create-app": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build && pnpm run test"
  },
  "keywords": [
    "cli",
    "template",
    "scaffold",
    "generator",
    "kitiumai",
    "typescript",
    "react"
  ],
  "author": "KitiumAI",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/create-app.git"
  },
  "bugs": {
    "url": "https://github.com/kitium-ai/create-app/issues"
  },
  "homepage": "https://github.com/kitium-ai/create-app#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
